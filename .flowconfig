[options]
suppress_comment=\\(.\\|\n\\)*\\$FlowExpectError
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
include_warnings=true
module.name_mapper='^firefox-profiler\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^firefox-profiler-res\/\(.*\)$' -> '<PROJECT_ROOT>/res/\1'
module.name_mapper.extension='ftl' -> '<PROJECT_ROOT>/src/types/mocks/ftl.js'

[ignore]
<PROJECT_ROOT>/node_modules/.*
<PROJECT_ROOT>/res/before-load\.js
<PROJECT_ROOT>/dist/.*

[libs]
src/types/libdef
src/types/globals
