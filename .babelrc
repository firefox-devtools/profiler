{
  presets: [
    [
      "env",
      {
        targets: {
          node: "current",
          browsers: "defaults"
        },
        modules: false
      }
    ],
    "react",
    "flow"
  ],
  plugins: [
    [
      "transform-runtime",
      {
        helpers: false,
        polyfill: false,
        regenerator: true
      }
    ],
    [
      "transform-class-properties"
    ]
  ]
}
