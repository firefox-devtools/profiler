    "globals": "^16.4.0",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^4.3.8",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-extended": "^6.0.0",
    "json-loader": "^0.5.7",
    "local-web-server": "^5.4.0",
    "lockfile-lint": "^4.14.1",
    "mkdirp": "^3.0.1",
    "npm-run-all2": "^8.0.4",
